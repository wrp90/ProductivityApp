  },
  "homepage": "https://github.com/wrp90/ProductivityApp#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jquery": "^3.6.0",
    "moment": "^2.29.1",
    "mongoose": "^5.12.11",
    "react-big-calendar": "^0.33.5",
    "react-bootstrap": "^1.6.0",
    "react-open-weather": "^1.1.5",
    "sequelize": "^6.6.2"
